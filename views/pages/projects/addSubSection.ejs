<form class="flex flex-col gap-[20px]" action="/api/projects/subSection/<%= id %>" method="POST" enctype="multipart/form-data">
    <div class="input flex flex-col gap-[15px]">
        <label class="text-[#f7a813] font-bold text-[20px]" for="name">Sub Section Name</label>
        <input class="bg-white outline-none w-full p-[20px] border-[2px] border-[#f7a813]" type="text" name="name" id="name" required>
    </div>
    <div class="input flex flex-col gap-[15px]">
        <label class="text-[#f7a813] font-bold text-[20px]" for="img">Sub Section Image</label>
        <input class="bg-white outline-none w-full p-[20px] border-[2px] border-[#f7a813]" type="file" name="img" id="img" required>
    </div>
    <input type="hidden" name="_csrf" value="<%= csrfToken %>">
    <input class="bg-[#f7a813] py-[15px] text-white cursor-pointer" type="submit" value="Add Sub Section">
</form>